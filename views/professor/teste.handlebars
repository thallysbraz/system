<div class="jumbotron">
    <h2>Lista de suas disciplinas teste:</h2>
    <hr>
    <table class="table table-striped table-bordered mt-3">
        <thead class="thead-dark">
            <tr>
                <th scope="col">
                    <center>
                        <h4>Alunos</h4>
                    </center>
                </th>
            </tr>
        </thead>
        <tbody>
            {{#each matricula}}
            <tr>
                <td>
                    <form action="/professor/notas/matricula" method="POST">
                        <label for="nota">Matricula: <strong>{{mat}}</strong></label>
                        <input type="hidden" name="id" value="{{mat}}">
                        <input type="text" id="nota" name="nota" placeholder="Nota" class="form-control">
                        <br>
                        <button class="btn btn-success" type="submit">
                            <i class="far fa-check-circle"> Salvar</i></button>
                    </form>
                </td>
            </tr>
            {{else}}
            {{/each}}
        </tbody>
    </table>
</div>